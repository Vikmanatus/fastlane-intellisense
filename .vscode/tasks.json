{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile",
			"group": "build",
			"presentation": {
				"panel": "dedicated",
				"reveal": "never"
			},
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"type": "npm",
			"script": "watch",
			"isBackground": true,
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"panel": "dedicated",
				"reveal": "never"
			},
			"problemMatcher": [
				"$tsc-watch"
			]
		},
		{
			"label": "webpack",
			"type": "shell",
			"command": "npm run webpack",
			"group": {
			  "kind": "build",
			  "isDefault": true
			},
			"presentation": {
			  "panel": "shared",
			  "reveal": "silent"
			},
			"problemMatcher": []
		  }
	]
}